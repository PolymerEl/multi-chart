  "scripts": {
    "start": "es-dev-server -v",
    "generate-docs-json": "wca analyze src/**/*.js  --outFiles docs/{tagname}.json ",
    "generate-docs-md": "wca analyze src/**/*.js  --outFiles docs/{tagname}.md ",
    "generate-docs:helper": "wca analyze src/helper/**/*.js --outFile docs/helper.json",
    "generate-docs:wrapper": "wca analyze src/d3-wrapper/**/*.js --outFile docs/wrapper.json",
    "generate-docs:chart": "wca analyze src/chart/**/*.js --outFile docs/chart.json",
    "generate-docs:container": "wca analyze src/container/**/*.js --outFile docs/container.json",
    "generate-docs": "npm run generate-docs-md && npm run generate-docs-json && npm run generate-docs:helper && npm run generate-docs:wrapper && npm run generate-docs:container && npm run generate-docs:chart",
    "prepublishOnly": "npm run generate-docs",
    "postpublish": "git push && git push --tags",
    "test": "karma start --coverage",
    "test:compatibility": "karma start --compatibility all --coverage"
  },
  "author": "christophe.geiser <christophe@preignition.org>"
}

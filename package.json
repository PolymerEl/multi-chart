    "d3-collection": "1",
    "d3-color": "^1.3.0",
    "d3-contour": "1",
    "d3-dispatch": "1",
    "d3-drag": "^1.2.4",
    "d3-dsv": "1",
    "d3-ease": "1",
    "d3-fetch": "1",
    "d3-force": "2",
    "d3-format": "1",
    "d3-geo": "^1.11.6",
    "d3-geo-projection": "^2.7.0",
